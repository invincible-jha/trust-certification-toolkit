{
  "name": "@aumos/badge-generator",
  "version": "0.1.0",
  "description": "SVG badge generator for the AumOS Certified badge program",
  "license": "Apache-2.0",
  "author": "MuVeraAI Corporation",
  "repository": {
    "type": "git",
    "url": "https://github.com/aumos-ai/trust-certification-toolkit.git",
    "directory": "badges/generator"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
